{
  "extends": "@moogur/tsconfig-frontend/index.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@api": ["./src/api/index.ts"],
      "@api/*": ["./src/api/*"],
      "@app": ["./src/app/index.ts"],
      "@assets/*": ["./src/assets/*"],
      "@pages": ["./src/pages/index.ts"],
      "@pages/*": ["./src/pages/*"],
      "@router": ["./src/router/index.ts"],
      "@shared/*": ["./src/shared/*"],
      "@store": ["./src/store/index.ts"],
      "@store/*": ["./src/store/*"],
      "@src/*": ["./src/*"]
    }
  },
  "references": [{ "path": "./tsconfig.node.json" }]
}
